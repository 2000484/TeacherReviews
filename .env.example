# Server Configuration
NODE_ENV=production
PORT=3000

# Logging
LOG_LEVEL=info

# Crocs Configuration
# Blacklist domains (regex patterns, comma-separated)
CROCS_BLACKLIST=example.com

# DNS Configuration  
# Comma-separated list of DNS servers
DNS_SERVERS=1.1.1.3,1.0.0.3

# Security
ALLOW_UDP_STREAMS=false

# WebSocket (Wisp) Configuration
WISP_ENABLE=true
